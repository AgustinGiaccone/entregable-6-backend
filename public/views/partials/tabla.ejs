<h1 style="text-align: center;">Vehiculos</h1>
<div class="table-responsive">
    <table class="table table-dark">
        <tr style="color: yellow;">
            <th>id</th>
            <th>Nombre</th>
            <th>Precio</th>
            <th>Imagen</th>
        </tr>
        <% listProd.forEach(prod => { %>
            <tr>
                <td> <%= prod.id %> </td>
                <td> <%= prod.title %> </td>
                <td> $<%= prod.price %> </td>
                <td> <img height="100" src="<%= prod.thumbnail %>" alt="<%= prod.title %>" width="140"> </td>
            </tr>
        <% }) %>
    </table>
</div>